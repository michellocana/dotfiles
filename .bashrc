# Paste this commands at the end of ~/.bashrc and restart command line to use this aliases

# Linux only
alias www='cd ~/www'

# Custom aliases
alias gst='git status'
alias gcm='git commit'
alias gpl='git pull'
alias gps='git push'
alias gadal='git add --all'
alias gd='git diff $'
alias gl='git log --pretty=oneline -n 20 --graph --abbrev-commit'
